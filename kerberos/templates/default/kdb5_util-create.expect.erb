#!/usr/bin/expect

set timeout -1
spawn $env(SHELL)
match_max 100000
send -- "/usr/kerberos/sbin/kdb5_util create -s"
expect -exact "/usr/kerberos/sbin/kdb5_util create -s"
send -- "\r"
expect "Enter KDC database master key: "
send -- "<%= @kdc_database_master_key %>\r"
expect "Re-enter KDC database master key to verify: "
send -- "<%= @kdc_database_master_key %>\r"
expect "\r"
send -- "exit\r"
expect eof
